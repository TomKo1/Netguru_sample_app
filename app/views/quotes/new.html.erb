<h1>Create new quote</h1>
<%= form_for @quote, url: quote_path(@quote) do |f| %>
    <table class="table table-striped">
        <tr>
            <%= f.label :quotation %>
            <%= f.text_area :quotation, cols: 20, rows: 10, class: 'form-control' %>
        <tr/>
        <tr>
            <%= f.submit 'Save', class: 'form-control btn-success' %>
        </tr>
    </table>
<% end %>